{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sairaj-ADF"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/SqlServerNew')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"parameters": {
					"servername": {
						"type": "string",
						"defaultValue": "LAPTOP-FDB4BSLC\\SAIRAJ"
					},
					"databasename": {
						"type": "string",
						"defaultValue": "AdventureWorks2012"
					},
					"username": {
						"type": "string",
						"defaultValue": "LAPTOP-FDB4BSLC\\saira"
					},
					"password": {
						"type": "string",
						"defaultValue": "236@Sairaj"
					}
				},
				"annotations": [],
				"type": "SqlServer",
				"typeProperties": {
					"connectionString": {
						"type": "SecureString",
						"value": "**********"
					},
					"userName": "@{linkedService().username}",
					"password": "@{linkedService().password}"
				},
				"connectVia": {
					"referenceName": "onpremdemoIR",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": []
		}
	]
}